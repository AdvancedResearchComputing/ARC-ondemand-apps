#!/bin/bash

pwd
#unset XDG_RUNTIME_DIR

module list

echo running on `hostname`
echo starting singularity

module load containers/singularity
module load MATLAB

## binding log files for troubleshooting
touch vnc.log
touch novnc.log

cat port.rc
	
bash start_container.sh ${TMPDIR}/<%= context.Container %> & 


sleep 15

matlab -desktop



